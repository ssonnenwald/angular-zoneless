{
  "workbench.colorTheme": "Visual Studio 2019 Dark",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "terminal.integrated.env.windows": {
    "NODE_OPTIONS": "--dns-result-order=ipv4first --max-old-space-size=8192"
  },
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "GitBash": {
      "source": "Git Bash",
      "path": ["C:\\Program Files\\Git\\bin\\bash.exe"],
      "icon": "terminal-bash"
    }
  },
  "git.confirmSync": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.scrollback": 10000,
  "[xml]": {
    "editor.defaultFormatter": "PrateekMahendrakar.prettyxml"
  },
  "diffEditor.maxComputationTime": 0,
  "explorer.compactFolders": false,
  "workbench.iconTheme": "material-icon-theme",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontFamily": "'Fira Code'",
  "editor.fontWeight": "500",
  "editor.foldingMaximumRegions": 6000,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[css]": {
    "editor.defaultFormatter": "michelemelluso.code-beautifier"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.advanced.externalDiffTool": "",
  "files.autoSave": "onFocusChange",
  "git.enableSmartCommit": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "debug.javascript.autoAttachFilter": "disabled",
  "codeium.enableSearch": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "codeium.snoozeTime": 1,
  "debug.allowBreakpointsEverywhere": true,
  "codeium.searchMaxWorkspaceFileCount": 0,
  "coverage-gutters.coverageFileNames": [
    "lcov.info",
    "cov.xml",
    "coverage.xml",
    "cobertura.xml",
    "jacoco.xml",
    "coverage.cobertura.xml"
  ],
  "cSpell.words": [
    "browserslistrc",
    "creational",
    "ghpages",
    "highlightjs",
    "Liskov",
    "MCSD",
    "mvvm",
    "stackoverflow"
  ],
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.fontLigatures.enabled": true,
  "search.exclude": {
    "**/docs": true
  }
}
